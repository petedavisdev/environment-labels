{
    "name": "Environment labels",
    "description": "Never mix up local, test and live environments",
    "action": {},
    "manifest_version": 3,
    "version": "0.1",
    "content_scripts": [
        {
            "matches": ["*://localhost*/*", "*://local*.**.**/*", "*://dev*.**.**/*", "*://deploy*.**.**/*", "*://preview*.**.**/*", "*://qa*.**.**/*", "*://uat*.**.**/*", "*://test*.**.**/*", "*://staging*.**.**/*"],
            "css": ["env-label.css"],
            "js": ["environmentLabels.js"]
        }
    ]
}
