{
	"name": "URL Marker",
	"version": "0.1",
	"description": "Never mix up local, test and live environments",
	"manifest_version": 3,
	"background": {
		"service_worker": "background.js"
	},
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"css": ["marker.css"],
			"js": ["marker.js"]
		}
	],
	"host_permissions": ["<all_urls>"],
	"options_page": "settings.html",
	"permissions": ["storage", "tabs"]
}
